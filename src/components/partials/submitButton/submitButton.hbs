<button class="submit-button {{#if submitButtonIconSrc}}submit-button-with-img{{/if}}" type="submit" id="{{name}}" name="{{name}}" {{#if hiddenInput}}hidden{{/if}}>
    {{#if submitButtonIconSrc}}
        <img class="img-for-submit-button" src="{{submitButtonIconSrc}}" alt="{{title}}">
    {{else}}
        {{title}}
    {{/if}}
    {{{spinner}}}
</button>